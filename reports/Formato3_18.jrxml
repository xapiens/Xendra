  <queryString><![CDATA[WITH
tax as (SELECT CASE WHEN $P{AD_Org_ID}=0 THEN max(ci.taxid) ELSE max(oi.taxid) END as id FROM ad_orginfo oi JOIN ad_clientinfo ci on ci.ad_client_id = oi.ad_client_id WHERE ci.ad_client_id = $P{AD_Client_ID}),
client as (SELECT CASE WHEN $P{AD_Org_ID}=0 THEN (SELECT description FROM AD_Client where AD_Client_ID = $P{AD_Client_ID}) ELSE (SELECT description FROM AD_org where AD_Org_ID = $P{AD_Org_ID}) END),
schema as (SELECT acc.c_acctschema_id, acc.name FROM c_acctschema acc WHERE acc.c_acctschema_id = $P{C_AcctSchema_ID}),
prevperiod as (SELECT c_period_id, name,enddate,periodno FROM c_period WHERE periodno < (SELECT periodno FROM c_period WHERE c_period_id = $P{C_Period_ID}) AND c_year_id = (SELECT c_year_id FROM c_period WHERE c_period_id = $P{C_Period_ID})),
currperiod as (SELECT c_period_id, name,enddate,periodno FROM c_period WHERE c_period_id = $P{C_Period_ID} ),
period as (SELECT c_period_id, name,enddate,periodno FROM c_period WHERE periodno <= (SELECT periodno FROM c_period WHERE c_period_id = $P{C_Period_ID} ) AND c_year_id = (SELECT c_year_id FROM c_period WHERE c_period_id = $P{C_Period_ID} ))
SELECT
-- título de reporte
(SELECT name FROM currperiod) as periodo,
(SELECT description FROM client) as cliente,
(SELECT id FROM tax)  as ruc,
(SELECT o.name FROM ad_org o WHERE o.ad_org_id = $P{AD_Org_ID} ) as organizacion,
(SELECT name FROM schema) as esquema,

-- saldo inicial
(         (SELECT SUM(f.amtacctdr) FROM fact_acct f
	JOIN prevperiod p on f.c_period_id = p.c_period_id
	JOIN c_elementvalue ev on f.account_id = ev.c_elementvalue_id
	WHERE f.ad_client_id = $P{AD_Client_ID}
	AND (CASE WHEN COALESCE($P{AD_Org_ID}, 0)  = 0
     	  THEN true
     	  ELSE f.ad_org_id = $P{AD_Org_ID} END)
	AND f.c_acctschema_id = $P{C_AcctSchema_ID}
	AND f.postingtype = $P{PostingType}
	AND left(ev.value,2) = '10') -
          (SELECT SUM(f.amtacctcr) FROM fact_acct f
	JOIN prevperiod p on f.c_period_id = p.c_period_id
	JOIN c_elementvalue ev on f.account_id = ev.c_elementvalue_id
	WHERE f.ad_client_id = $P{AD_Client_ID}
	AND (CASE WHEN COALESCE($P{AD_Org_ID}, 0)  = 0
    	   THEN true
    	   ELSE f.ad_org_id = $P{AD_Org_ID} END)
	AND f.c_acctschema_id = $P{C_AcctSchema_ID}
	AND left(ev.value,2) = '10')
) as inicial,

--cobranza de ventas
(	(SELECT SUM(f.amtacctdr) FROM fact_acct f
	JOIN currperiod p on f.c_period_id = p.c_period_id
	JOIN c_elementvalue ev on f.account_id = ev.c_elementvalue_id
	LEFT JOIN c_payment py on f.record_id = py.c_payment_id
	LEFT JOIN c_cashflow_group flow on py.c_cashflow_group_id = flow.c_cashflow_group_id
	LEFT JOIN c_allocationline allo on f.record_id = allo.c_allocationhdr_id
	LEFT JOIN c_payment po on allo.c_payment_id = po.c_payment_id
	LEFT JOIN c_cashflow_group flow2 on po.c_cashflow_group_id = flow2.c_cashflow_group_id
	LEFT JOIN c_cashline ch on f.record_id = ch.c_cash_id
	LEFT JOIN c_cashflow_group flow3 on ch.c_cashflow_group_id = flow3.c_cashflow_group_id
	WHERE f.ad_client_id = $P{AD_Client_ID}
	AND (CASE WHEN COALESCE($P{AD_Org_ID}, 0)  = 0
     	  THEN true
     	  ELSE f.ad_org_id = $P{AD_Org_ID} END)
	AND f.c_acctschema_id = $P{C_AcctSchema_ID}
	AND f.postingtype = $P{PostingType}
	AND ev.value like '10%'
	AND (flow.value = 'OP+01'
	OR flow2.value = 'OP+01'
	OR flow3.value = 'OP+01')
	AND (CASE WHEN COALESCE($P{DateAcct},(SELECT enddate FROM currperiod))=(SELECT enddate FROM currperiod)
      	THEN true
      	ELSE f.dateacct <= $P{DateAcct} END))
      	-
	(SELECT SUM(f.amtacctcr) FROM fact_acct f
	JOIN currperiod p on f.c_period_id = p.c_period_id
	JOIN c_elementvalue ev on f.account_id = ev.c_elementvalue_id
	LEFT JOIN c_payment py on f.record_id = py.c_payment_id
	LEFT JOIN c_cashflow_group flow on py.c_cashflow_group_id = flow.c_cashflow_group_id
	LEFT JOIN c_allocationline allo on f.record_id = allo.c_allocationhdr_id
	LEFT JOIN c_payment po on allo.c_payment_id = po.c_payment_id
	LEFT JOIN c_cashflow_group flow2 on po.c_cashflow_group_id = flow2.c_cashflow_group_id
	LEFT JOIN c_cashline ch on f.record_id = ch.c_cash_id
	LEFT JOIN c_cashflow_group flow3 on ch.c_cashflow_group_id = flow3.c_cashflow_group_id
	WHERE f.ad_client_id = $P{AD_Client_ID}
	AND (CASE WHEN COALESCE($P{AD_Org_ID}, 0)  = 0
    	   THEN true
     	   ELSE f.ad_org_id = $P{AD_Org_ID} END)
	AND f.c_acctschema_id = $P{C_AcctSchema_ID}
	AND f.postingtype = $P{PostingType}
	AND ev.value like '10%'
	AND (flow.value = 'OP+01'
	OR flow2.value = 'OP+01'
	OR flow3.value = 'OP+01')
	AND (CASE WHEN COALESCE($P{DateAcct},(SELECT enddate FROM currperiod))=(SELECT enddate FROM currperiod)
       	THEN true
      	ELSE f.dateacct <= $P{DateAcct} END))
) as cob_vent,

